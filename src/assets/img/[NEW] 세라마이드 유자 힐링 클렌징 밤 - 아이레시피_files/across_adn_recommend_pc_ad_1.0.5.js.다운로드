													$("#"+suggest_wrap_id).append("<div id='suggestad_wrap_sub_container_"+adn_suggestad_grp_ix+"'  ></div>");
												}
												
												var suggest_sub_wrap_id = "suggestad_wrap_sub_container_"+adn_suggestad_grp_ix;
												$("#"+suggest_sub_wrap_id).html(data.infos );   
												      
												if(adn_suggestad_align==2)
												{
													page_count = $("#adn_fixed_banner_area_"+adn_suggestad_grp_ix).data("pagecount");
													if(page_count)
													{
														arr_page_count.push({adn_suggestad_grp_ix, page_count, adn_suggestad_rows});

														      
														page_num = 1
														arr_page_num.push({adn_suggestad_grp_ix, page_num, adn_suggestad_rows});

													}
													      
													var half_screen1 = parseInt(document.body.clientWidth/2);
													var half_screen = parseInt(document.body.scrollWidth/2);
													var pos_left = parseInt(adn_suggestad_poz_x);
													
													var drag_posX = half_screen - pos_left;
													if(parseInt(drag_posX)<0) drag_posX = 0;
													
													$("#adn_fixed_banner_area_"+adn_suggestad_grp_ix ).css("top", adn_suggestad_poz_y + "px");
													
													if(drag_posX!="0"){
														$("#adn_fixed_banner_area_"+adn_suggestad_grp_ix ).css("left", drag_posX + "px");
													}else{
														$("#adn_fixed_banner_area_"+adn_suggestad_grp_ix ).css("left", "100px");
													}
													
													$("#adn_fixed_banner_area_"+adn_suggestad_grp_ix).css({"border":"1px solid "+adn_suggestad_border_colors});
													$("#adn_fixed_banner_area_"+adn_suggestad_grp_ix+" .thumb_box").css({"border":"1px solid "+adn_suggestad_goods_colors});
													
													if(adn_suggestad_rows==3)
													{
														$(".adn_slide_goods_list").hide();
														$(".goods_row3_page_1").show();
													}
												}
												else
												{
													if(adn_suggestad_types==3) $("#"+suggest_sub_wrap_id+ ".adn_paging_number").css("margin-top", " -5px");
													else $("#"+suggest_sub_wrap_id+ ".adn_paging_number").css("margin", " -30px 25px 0px 0px");
													
													if(adn_suggestad_borders==1)
													{
														$("#"+suggest_sub_wrap_id+ " div.adn_banner_wrap").css({"border":"1px solid "+adn_suggestad_border_colors, "padding":"15px 15px 0px 15px"});
													}
													if(adn_suggestad_goods_borders==1)
													{
														$("#"+suggest_sub_wrap_id+ ".thumb_box").css({"border":"1px solid "+adn_suggestad_goods_colors});
													}
												}
												
												$("#"+suggest_sub_wrap_id).css({"padding":"0px"});
												      
												$("#"+suggest_sub_wrap_id).show();
												      
												$("#"+suggest_sub_wrap_id+ " .now_page_num").text(1);
												      
												if(adn_suggestad_align==1)
												{
													
													if(adn_suggestad_borders==1)
													{
														$("#"+suggest_sub_wrap_id+ " div.adn_banner_wrap").css({"border":"1px solid "+adn_suggestad_border_colors, "padding":"15px 15px 0px 15px"});
													}
													if(adn_suggestad_goods_borders==1)
													{
														$("#"+suggest_sub_wrap_id+ ".thumb_box").css({"border":"1px solid "+adn_suggestad_goods_colors});
													}
													
													var tab_id = "adn_hash_tab01_"+adn_suggestad_grp_ix;
													var tab02_id = "adn_hash_tab01_type02_"+adn_suggestad_grp_ix;
													var tab03_id = "adn_hash_tab01_type03_"+adn_suggestad_grp_ix;

													
													if(adn_suggestad_types==4)
													{
														if (document.getElementById(tab02_id)) 
														{
															page_count = $("#"+tab02_id).data("pagecount");
															if(page_count)
															{
																arr_page_count.push({adn_suggestad_grp_ix, page_count, adn_suggestad_rows});
