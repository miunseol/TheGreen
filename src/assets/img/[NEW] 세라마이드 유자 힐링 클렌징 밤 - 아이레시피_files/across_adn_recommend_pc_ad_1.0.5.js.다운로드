								} catch (e) {}

							
								if(/Mobi/.test(navigator.userAgent)==false)
								{
									$.ajax({
										url: p_url,    
										type: "GET",
										crossDomain: true,
										data: { goods: adn_goods_items_info, sw: adn_shop_search_info },
										dataType: "jsonp",  
										success:function(data){
											if (data.status == "0") {
											} else {
												var adn_suggestad_camp_code = data.camps;
												var adn_suggestad_grp_ix = data.grp_ix;
												 var adn_bottom_status = data.bottom;
												var adn_suggestad_types = data.types;
												var adn_suggestad_types_1st = data.types_1st;
												var adn_suggestad_types_2st = data.types_2st;
