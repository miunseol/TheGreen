												var adn_suggestad_close_rate = data.close_rate;
												var adn_suggestad_rows = data.rows;
												var adn_suggestad_rolling = data.rolling;
												var adn_suggestad_rolling_sec = data.rolling_sec;
												var adn_suggestad_borders = data.borders;
												var adn_suggestad_border_colors = data.border_colors;
												var adn_suggestad_goods_borders = data.goods_borders;
												var adn_suggestad_goods_colors = data.goods_colors;
												                  

												if(adn_suggestad_align==1)
												{
													$("#"+suggest_wrap_id).append("<div id='suggestad_wrap_sub_container_"+adn_suggestad_grp_ix+"'  style='margin:auto;max-width:1200px;'></div>");
												}
												else
