																		if(tmp_grp_ix==value["adn_suggestad_grp_ix"])
																		{
																			
																			arr_page_num[key]["page_num"] = now_page_num;
																		}
																	});
																}
																if(adn_suggestad_align==1){

																	$("#"+adn_suggestad_active_id+" .goods_page_items").hide();
																	$("#"+adn_suggestad_active_id+" .goods_page_type03_"+now_page_num).show();
																	$("#suggestad_wrap_sub_container_"+tmp_grp_ix+" .now_page_num_type03").text(now_page_num);
																}else{
																	
																	if(now_goods_list_rows==1){
																		$("#"+wrap_ids+" > div.goods_list_box > div.adn_fixed_list.adn_items_list").hide();
																		$("#"+wrap_ids+" > div.goods_list_box > div.adn_fixed_list.adn_items_list.goods_page_"+now_page_num).show();
																	}else if(now_goods_list_rows==3){
																		$("#"+wrap_ids+" > div.goods_list_box > div.adn_slide_goods_list").hide();
																		$("#"+wrap_ids+" > div.goods_list_box > div.adn_slide_goods_list.goods_page_"+now_page_num).show();
																	}
																	$("#"+wrap_ids+" .now_page_num").text(now_page_num);
																}
															}
														}
													});
												}catch(e){}
												
												try{
													$("body").delegate("#"+suggest_sub_wrap_id+" .adn_banner_wrap .tab_category_type02 li", "click", function() {
														if(adn_suggestad_align==1)
														{
															$("#"+suggest_sub_wrap_id+" .adn_banner_wrap .tab_category_type02 li").removeClass("active").css("color", "#8f8f8f");
															$(this).addClass("active").css("color", "#f24d4d");
															$("#"+suggest_sub_wrap_id+" .adn_banner_wrap .tab_content_type02").hide();
															
															var activeTab = $(this).attr("rel");
															$("#" + activeTab).fadeIn();
															
															page_count = $("#"+activeTab).data("pagecount");
															
															var tmp_id = activeTab.split("_");
															

															if(tmp_id.length>0)
															{
																tmp_grp_ix = tmp_id[tmp_id.length-1];
															}
															if(Array.isArray(arr_page_count))
															{
																jQuery.each(arr_page_count,function(key,value) {
																	if(tmp_grp_ix==value["adn_suggestad_grp_ix"])
																	{
																		arr_page_count[key]["page_count"] = page_count;
																	}
																});
															}
															if(Array.isArray(arr_page_num))
															{
																jQuery.each(arr_page_num,function(key,value) {
																	if(tmp_grp_ix==value["adn_suggestad_grp_ix"])
																	{
																		arr_page_num[key]["page_num"] = 1;
																	}
																});
															}
															
															$("#"+suggest_sub_wrap_id+" .adn_banner_wrap .tab_content_type02 .goods_page_items").hide();
															
															page_num = 1; 
															$("#"+suggest_sub_wrap_id+" .now_page_num_type02").text(1);  
															$("#"+suggest_sub_wrap_id+" .page_total_type02").text(page_count);
															$("#"+suggest_sub_wrap_id+" .goods_page_type02_1").show();
															
															adn_suggestad_active_id = activeTab;
															if(adn_suggestad_rolling==1)
															{
																if(adn_suggestad_types==3){
																
																	var sliide_show_num = 3;
																	var sliide_scroll_num = 1;
																
																}else{
																	var sliide_show_num = 5;
																	var sliide_scroll_num = 1;
																}
																//}else{
																//var sliide_show_num = 10;
																//var sliide_scroll_num = 2;
																//}
																var tmp_ids = adn_suggestad_active_id.split("_");
																var tmp_grp_ix = tmp_ids[tmp_ids.length-1];
																
																
																goods_slide_status =$("#"+adn_suggestad_active_id+" .goods_slider_wrap .goods_list_box").slick({
																	dots: true,
																	autoplay: true,
																	infinite: true,
																	autoplaySpeed: 3000,
																	slidesToShow: sliide_show_num,
																	slideswToScroll: 1,
																	centerMode: false,
																	arrows: true,
																	waitForAnimate:true,
																	pauseOnHover:true,
																	swipe:false,
																	fade:false,
																	prevArrow: $("#slide_left_btn_"+tmp_grp_ix),
																	nextArrow: $("#slide_right_btn_"+tmp_grp_ix),
																});
																$(".slick-dots").css("display","none");
																$("#"+adn_suggestad_active_id+" .goods_slider_wrap .goods_list_box").show();
															}
														}
													});
												}catch(e){}
												
												
												try{
													$("body").delegate("#"+suggest_sub_wrap_id+" .adn_banner_wrap .tab_category_type03 li", "click", function() {
														if(adn_suggestad_align==1)
														{
															$("#"+suggest_sub_wrap_id+" .adn_banner_wrap .tab_category_type03 li").removeClass("active").css("color", "#8f8f8f");
															$(this).addClass("active").css("color", "#f24d4d");
															$("#"+suggest_sub_wrap_id+" .adn_banner_wrap .tab_content_type03").hide();
															
															var activeTab = $(this).attr("rel");
															$("#" + activeTab).fadeIn();
															
															page_count = $("#"+activeTab).data("pagecount");
															
															var tmp_id = activeTab.split("_");
															
															if(tmp_id.length>0)
															{
																tmp_grp_ix = tmp_id[tmp_id.length-1];
															}
															if(Array.isArray(arr_page_count))
															{
																jQuery.each(arr_page_count,function(key,value) {
																	if(tmp_grp_ix==value["adn_suggestad_grp_ix"])
																	{
																		arr_page_count[key]["page_count"] = page_count;
																	}
																});
															}
															if(Array.isArray(arr_page_num))
															{
																jQuery.each(arr_page_num,function(key,value) {
																	if(tmp_grp_ix==value["adn_suggestad_grp_ix"])
																	{

																		arr_page_num[key]["page_num"] = 1;
																	}
																});
															}
															
															$("#"+suggest_sub_wrap_id+" .adn_banner_wrap .tab_content_type03 .goods_page_items").hide();
															
															page_num = 1; 
															$("#"+suggest_sub_wrap_id+" .now_page_num_type03").text(1);  
															$("#"+suggest_sub_wrap_id+" .page_total_type03").text(page_count);
															$("#"+suggest_sub_wrap_id+" .goods_page_type03_1").show();
															
															adn_suggestad_active_id = activeTab;
															if(adn_suggestad_rolling==1)
															{
																if(adn_suggestad_types==3){
																
																	var sliide_show_num = 3;
																	var sliide_scroll_num = 1;
																
																}else{
																	var sliide_show_num = 5;
																	var sliide_scroll_num = 1;
																}
																//}else{
																//var sliide_show_num = 10;
																//var sliide_scroll_num = 2;
																//}
																var tmp_ids = adn_suggestad_active_id.split("_");
																var tmp_grp_ix = tmp_ids[tmp_ids.length-1];
																
																
																goods_slide_status =$("#"+adn_suggestad_active_id+" .goods_slider_wrap .goods_list_box").slick({
																	dots: true,
																	autoplay: true,
																	infinite: true,
																	autoplaySpeed: 3000,
																	slidesToShow: sliide_show_num,
																	slideswToScroll: 1,
																	centerMode: false,
																	arrows: true,
																	waitForAnimate:true,
																	pauseOnHover:true,
																	swipe:false,
																	fade:false,
																	prevArrow: $("#slide_left_btn_"+tmp_grp_ix),
																	nextArrow: $("#slide_right_btn_"+tmp_grp_ix),
																});
																$(".slick-dots").css("display","none");
																$("#"+adn_suggestad_active_id+" .goods_slider_wrap .goods_list_box").show();
															}
														}
													});
												}catch(e){}
												
												$("body").delegate("#"+suggest_sub_wrap_id+" .adn_text_box button", "mouseenter", function() {
													if(adn_suggestad_align==1)
													{
														$("#"+suggest_sub_wrap_id+" .adn_event_banner_wrap .adn_event .adn_text_box button").removeClass("active").css("color", "#ffffff");
														var active_id = $(this).attr("id");
														
														
														if(active_id){
															var active_thumb_id = $(this).attr("id").replace("_txt","");
															
															$("#"+active_id).addClass("active");
															if($("#"+active_thumb_id).css("display")=="none")
															{
																$("#"+suggest_sub_wrap_id+" .adn_event_banner_wrap .adn_event .adn_image_box .adn_event_images").hide();
																$("#"+active_thumb_id).fadeIn(0);
															}
														}
													}
												});
										  }
										},
										error:function(xhr,status,error){
										   console.log(status)
										}
									});  
									
									
									
								}

							}
						}
					} catch (e) {}
				}		
		});
});	